<body>
    
    <!-- Wrapper for all page content EXCEPT (sticky) footer -->
    <div id="wrap">

        <!-- Loads navbar (menu) -->
        @Html.Partial("_NavBar")

        <!-- Jumbotron/Featured -->
        @RenderSection("featured", required: false)

        <!-- Begin page content, loads VIEWs -->
        <div class="container">
            @RenderBody()
        </div>

        <!-- Pusher for sticky/fixed footer, must be same HEIGHT as #footer -->
        <div id="push"></div>

    </div>

    <!-- Loads footer, has #footer div inside -->
    @Html.Partial("_FooterBar")

    @RenderSection("scripts", required: false)

</body>
</html>
